{
  "type": "object",
  "properties": {
    "islogin": {
      "type": "identifier",
      "selector": "body",
      "control": "exists"
    },
    "username": {
      "type": "string",
      "control": "property.innerText",
      "selector": "body > div:nth-child(5) > form > table > tbody > tr:nth-child(2) > td:nth-child(2) > input[type=\"text\"]"
    },
    "password": {
      "type": "string",
      "control": "text",
      "selector": "[name=\"password\"]"
    },
    "submit": {
      "type": "action",
      "selector": "body > div:nth-child(5) > form > table > tbody > tr:nth-child(6) > td > input",
      "control": "property.innerHTML"
    }
  },
  "connect": [
    {
      "match": [
        "islogin"
      ],
      "screens": [
        "login"
      ]
    }
  ]
}
